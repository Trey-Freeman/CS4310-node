    "fdom": "^1.0.0",
    "kgo": "^2.0.0",
    "rtc-attach": "^2.0.1",
    "rtc-capture": "^1.0.1",
    "rtc-quickconnect": "^4.1.0",
    "whisk": "^1.0.0"
  },
